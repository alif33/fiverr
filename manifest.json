 {
    "name": "Fiverr intelligence.",
    "version": "1.0",
    "description": "Boost your power then human.",
    "browser_action": {
          "default_popup": "popup.html",
          "default_icon": {
            "16": "img/icon.png",
            "32": "img/icon.png",
            "48": "img/icon.png",
            "128": "img/icon.png"
          }
      },
    "icons": {
         "16": "img/icon.png",
         "32": "img/icon.png",
         "48": "img/icon.png",
         "128": "img/icon.png"
     },
    "content_scripts": [
     {
       "matches": ["http://*/*" , "https://*/*"],
       "css": ["myStyles.css"],
       "js": ["contentScript.js"]
     }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
      "storage",
      "tabs"
    ],
    "manifest_version": 2
  }